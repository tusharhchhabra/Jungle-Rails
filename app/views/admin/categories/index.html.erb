<h1 class="mt-4">Categories</h1>
<div class="well my-4">
    <%= link_to '+ New Category', [:new, :admin, :category], class: 'btn btn-info' %>
</div>

<table>
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
    <% @categories.each do |category| %>
      <tr>
        <td><%= category.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
